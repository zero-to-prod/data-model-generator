  "keywords": [
    "zero-to-prod",
    "DataModelGenerator",
    "data-model-generator"
  ],
  "homepage": "https://github.com/zero-to-prod/data-model-generator",
  "license": "MIT",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "zero-to-prod/data-model-helper": "^81.1",
    "zero-to-prod/transformable": "^71.0",
    "zero-to-prod/data-model-factory": "^71.1"
  },
  "autoload": {
    "psr-4": {
      "Zerotoprod\\DataModelGenerator\\": "src/"
